{
  "name": "phore/jwt",
  "description": "Phore JWT library",
  "license": "MIT",
  "homepage": "https://infracamp.org/project/phore",
  "authors": [
    { "name": "Matthias Leuffen", "email": "m@tth.es" },
    { "name": "Michael Schaar"}
  ],
  "repositories": [
    {
        "type": "vcs",
        "url":  "git@github.com:phore/phore-asn-der.git"
    }
],
  "require": {
    "php": ">=7.2",
    "ext-json": "*",
    "ext-openssl": "*",
    "phore/cache": "dev-master",
    "phore/http-client": "dev-master",
    "phore/objectstore": "dev-master"
  },
  "suggest": {
  },
  "autoload": {
    "psr-4": {
      "Phore\\JWT\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": { "Phore\\Tests\\": "tests/" }
  },
  "minimum-stability": "dev",
  "require-dev": {
    "phpunit/phpunit": "*",
    "phore/phore-asn-der": "*"
  }
}
